package parsers;

public class ParserEx1 {

	public static void main(String[] args) {
		XMLParser xp = new XMLParser();
		JSONParser jp = new JSONParser();
		
		ParseManager pm = new ParseManager ();
		pm.parsingData(xp);
		pm.stopParsing(xp);
		
		pm.parsingData(jp);
		pm.stopParsing(jp);
		

	}

}

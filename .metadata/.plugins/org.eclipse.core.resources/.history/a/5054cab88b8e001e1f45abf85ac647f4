package collection;

public class TreeSetTest {

	public static void main(String[] args) {

		// 반 생성
		ClassRoom class1 = new ClassRoom(1);
		
		Student s1 = new Student ("23001", "둘리", 30);
		Student s2 = new Student ("23002", "또치", 90);
		Student s3 = new Student ("23003", "희동", 68);
		Student s4 = new Student ("23004", "길동", 45);
		
		class1.addStudentsByScore(s1); // collection.Student cannot be cast to java.lang.Comparable
		class1.addStudentsByScore(s2);
		class1.addStudentsByScore(s3);
		class1.addStudentsByScore(s4);
		
		class1.outputStudent();
		
		
		ClassRoom class2 = new ClassRoom(2);
		
		Student s5 = new Student ("23001", "둘리", 30);
		Student s6 = new Student ("23002", "또치", 90);
		Student s7 = new Student ("23003", "희동", 68);
		Student s8 = new Student ("23004", "길동", 45);
		
		class2.addStudentsByScore(s5); // collection.Student cannot be cast to java.lang.Comparable
		class2.addStudentsByScore(s6);
		class2.addStudentsByScore(s7);
		class2.addStudentsByScore(s8);
		
		class2.outputStudent();
		
	}
}

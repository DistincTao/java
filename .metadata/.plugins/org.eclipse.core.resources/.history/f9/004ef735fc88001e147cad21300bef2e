package department;

import employees.Employees;
import employees.Part;

public class Accountant {
	private String name;
	
	public Accountant(String name) {
		this.name = name;
	}

	public String getName() {
		return this.name;
	}
	
	public void calPay (Department d) {
		for (int i = 0; i < d.getMember().length; i++) {
			if (d.member[i] == null) break;
			if (d.member[i].getType().equals("PartTime")) {
				System.out.println(d.member[i].toString());
			} else {
				System.out.println(d.member[i].toString());
			}
		}
	}
	

	
	
	
	
}

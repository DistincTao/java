package department;

public class Accountant {
	private String name;
	
	Accountant(String name){
		this.name = name;
	}
	public void calPay (Department d) {
		for (int i =0; i < d.getMember().length; i++) {
			if (d.getMember()[i] != null) {
				System.out.println((i + 1) + d.getMember()[i].toString());

			}
		}
	}

}

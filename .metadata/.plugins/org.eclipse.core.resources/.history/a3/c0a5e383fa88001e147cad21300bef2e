package department;

import employees.Employees;

public class SaleTeam extends Department{
	private Employees[] member = new Employees[10];
	int i = 0;

	public SaleTeam(String name) {
		super(name);
	}

	public void recordMemebers(Employees e) {
		for (int i = 0; i < member.length; i++) {
			if (member[i] == null) break;
		}
		member[i++] = e;
	}
}
package parsers;

public class ParseManager{	
	
	// 매소드
	public static Parsable getParser(String fileName) {

		if (fileName.equals("xml")) {
			String txt = fileName.substring(fileName.indexOf("xml"));
			return new XMLParser();
			
		} 
		if (fileName.equals("json")) {
			String txt = fileName.substring(fileName.indexOf("json"));
			return new JSONParser();
			
		} 
		
		
		
	}
	
}

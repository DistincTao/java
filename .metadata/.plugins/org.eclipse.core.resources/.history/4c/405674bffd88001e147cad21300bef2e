package department;

import employees.Formal;
import employees.Part;

public class CompanyEx1 {
	public static void main(String[] args) {
		
		Department salesTeam = new Department("영업팀");
		salesTeam.addMemebers(new Formal("둘리", "Full", 1500000));
		salesTeam.addMemebers(new Part("희동", "Part", 12, 20));
		
		for (int i =0; i < salesTeam.getMember().length; i++) {
			salesTeam.getMember()[i].toString();
		}

	}
}

package collection;

import java.util.HashMap;
import java.util.Map;

public class HashMapTest {

	public static void main(String[] args) {
		Map<Integer, String> map = new HashMap <> ();
		
		map.put(1, "하영");
		map.put(2, "연지");
		map.put(3, "지우");
		map.put(4, "수혁");
		map.put(5, "동진");
		map.put(6, "현근");
		map.put(7, "상용");
		map.put(7, "현성");
		
		// 데이터 탐색
		
		if (map.containsKey(7)) {
			System.out.println(map.get(7));
		}
		

	}

}
